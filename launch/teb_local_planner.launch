<?xml version="1.0"?>
<launch>

  <!-- Move_base-->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find dualarm_mobile_planner)/params/costmap_common_params.yaml"     command="load" ns="global_costmap" />
    <rosparam file="$(find dualarm_mobile_planner)/params/costmap_common_params.yaml"     command="load" ns="local_costmap" />
    <rosparam file="$(find dualarm_mobile_planner)/params/local_costmap_params.yaml"      command="load" />
    <rosparam file="$(find dualarm_mobile_planner)/params/global_costmap_params.yaml"     command="load" />
    <rosparam file="$(find dualarm_mobile_planner)/params/teb_local_planner_params.yaml"  command="load" />
    <!--<rosparam file="$(find dualarm_mobile_planner)/params/costmap_converter_params.yaml"  command="load" />-->
  
    <param name="base_global_planner"   value="global_planner/GlobalPlanner" />
    <param name="planner_frequency"     value="10.0" />
    <param name="planner_patience"      value="15.0" />

    <param name="base_local_planner"    value="teb_local_planner/TebLocalPlannerROS" />
    <param name="controller_frequency"  value="10.0" />
    <param name="controller_patience"   value="15.0" />
  </node>
  
</launch>
